msgid "Texturing mode"
msgstr ""

#: ../../reference_manual/brushes/brush_settings/texture.rst:52
msgid "Cutoff policy will determine what range and where the strength will affect the textured outcome."
msgstr ""

#: ../../reference_manual/brushes/brush_settings/texture.rst:54
msgid "Disabled"
msgstr ""

#: ../../reference_manual/brushes/brush_settings/texture.rst:55
msgid "Doesn't cut off. Full range will be used."
msgstr ""

